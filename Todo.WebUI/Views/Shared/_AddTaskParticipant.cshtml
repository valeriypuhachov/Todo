@using Todo.WebUI.Models
@model AddTaskParticipantViewModel

<div class="row">

    <div class="col-md-1">
        @Html.CheckBoxFor(x => x.IsChecked)
    </div>
    <div class="col-md-4 col-md-offset-3">
        @Html.Label("FullName", $"{Model.FullName}")
    </div>
    <div class="col-md-1">
        <input type="submit" class="btn btn-default" value="Remove"/>
    </div>
</div>
<hr/>